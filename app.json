{
  "expo": {
    "name": "KW - Alyfe",
    "slug": "kw-app",
    "version": "1.4.2",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-kw.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.66cedric.lopez.kwapp"
    },
    "android": {
      "permissions": [
        "INTERNET"
      ],
      "versionCode": 36,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kw.mymarketing",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "9d67805e-2b81-422e-9a30-f6cd6c7f0d63"
      },
      "plugins": [
        [
          "expo-contacts",
          {
            "contactsPermission": "Autoriser Kw Alyfe à accéder à vos contacts"
          }
        ]
      ]
    }
  }
}